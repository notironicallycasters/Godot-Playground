config_version=5

[application]

config/name="Godot Playground"
run/main_scene="res://scenes/maps/Baseline.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

RigidBodyAutoScaler="*res://addons/rigidbody_auto_scaler/RigidBodyAutoScaler.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/always_on_top=true
window/size/transparent=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-box2d/plugin.cfg", "res://addons/rigidbody_auto_scaler/plugin.cfg", "res://addons/ropesim/plugin.cfg", "res://addons/softbody2d/plugin.cfg")

